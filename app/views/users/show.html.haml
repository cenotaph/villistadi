.project_container.user_profile
  .row
    .small-12.columns
      %h3= @user.name
    
  .row
    .small-12.columns
      = t :belongs_to_projects
      
  .row
    .small-12.columns
      - if @user.projects.empty?
        = t :no_projects
      - else
        - @user.projects.each do |proj|
          .project= render :partial => 'projects/project', locals: {project: proj }
